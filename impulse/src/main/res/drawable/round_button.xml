<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape android:shape="oval">
            <stroke
                android:width="1dp"
                android:color="#000000" />
        </shape>
    </item>
    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape android:shape="oval" >
            <stroke
                android:width=".5dp"
                android:color="#FFFFFF" />
        </shape>
    </item>

</layer-list>

